{
    "version": 1,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": ["GET", "PUT", "PATCH", "POST", "DELETE", "OPTIONS"]
        }
    ]
}